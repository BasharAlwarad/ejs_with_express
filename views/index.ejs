<!DOCTYPE html>
<html>
<head>
  <title>Static Index Page with CSS</title>
  <link rel="stylesheet" type="text/css" href="/css/styles.css">
</head>
<body>
  <h1>Users</h1>
  <ul>
    <% users.forEach(function(user) { %>
      <li><%= user.name %> - <%= user.email %></li>
    <% }); %>
  </ul>
  <p>
    <%= message %>
  </p>
  <script src="/js/script.js"></script>
</body>
</html>
